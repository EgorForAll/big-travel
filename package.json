{
  "name": "big-trip",
  "version": "15.0.0",
  "private": true,
  "description": "Проект «{{projectTitle}}» от HTML Academy",
  "scripts": {
    "eslint": "eslint src/",
    "test": "npm run eslint",
    "build": "webpack --mode development",
    "start": "webpack-dev-server --open"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "8.36.0",
    "eslint-config-htmlacademy": "9.0.0",
    "nanoid": "4.0.2",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.12.0"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "dayjs": "1.9.5",
    "http-proxy-middleware": "^2.0.6"
  }
}
